#ifndef TrajectoryPlanner_idl
#define TrajectoryPlanner_idl
/*
#include "BasicDataType.idl"
#include "ExtendedDataTypes.idl"
#include "InterfaceDataTypes.idl"
*/
module RTC {

  enum RETURN_VALUE {
    RETVAL_OK,
    RETVAL_INVALID_PARAMETER,
    RETVAL_EMPTY_MAP,
    RETVAL_INVALID_PRECONDITION,
    RETVAL_NOT_IMPL,
    RETVAL_UNKNOWN_ERROR
  };
  
  const long jointNum = 7;
  const long maxWayPoints = 100;

  typedef double jointPos[jointNum];
  typedef double jPosTraj[maxWayPoints][jointNum];

  interface TrajectoryPlanner {
    RETURN_VALUE planTrajectory(in jointPos start, in jointPos goal, out jPosTraj trajectory);
};
  

};


#endif
